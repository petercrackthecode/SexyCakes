{
  "expo": {
    "name": "Say Mile",
    "slug": "say-mile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/saymile.png",
    "splash": {
      "image": "./assets/saymile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg",
        "svgx"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "saymile.ios"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Test flight\n",
    "android": {
      "package": "saymile.ios"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
